{
  "name": "reason-java-ffi",
  "esy": {
    "build": "dune build -p #{self.name}",
    "install": "esy-installer #{self.name}.install"
  },
  "dependencies": {
    "@esy-ocaml/reason": "~3.6.0",
    "@opam/dune": "~2.6.1",
    "@opam/javalib": "3.6.1",
    "@opam/ocaml-migrate-parsetree": "1.7.3",
    "@opam/ppx_deriving": "4.5",
    "@opam/ppx_tools_versioned": "5.4.0",
    "@opam/ppxlib": "0.14.0",
    "camljava": "EduardoRFS/camljava:package.json",
    "ocaml": "~4.10.0"
  },
  "devDependencies": {
    "@opam/ocaml-lsp-server": "github:ocaml/ocaml-lsp:ocaml-lsp-server.opam#c1e51a50a85aacf2bf0479e7c8f8fd1a9fc977c2"
  },
  "resolutions": {
    "@opam/javalib": {
      "source": "EduardoRFS/javalib:javalib.opam#5d597e405c1cb0c5123f3376dd21a4fed1798971",
      "override": {
        "build": [
          ["./configure.sh"],
          ["make", "OCAMLPATH=$OCAMLPATH"]
        ],
        "install": [
          ["make", "install", "OCAMLFIND_DESTDIR=$OCAMLFIND_DESTDIR"]
        ]
      }
    }
  }
}
